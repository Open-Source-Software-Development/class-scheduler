<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "osd.database.UserConstraintRecord" table = "scheduler_userconstraint">

        <id name = "id" type = "int" column = "ID">
            <generator class="native"/>
        </id>

        <property name="leftTypeId" column="LEFT_ARGUMENT_TYPE_ID" type="int" />
        <property name="rightTypeId" column="RIGHT_ARGUMENT_TYPE_ID" type="int" />
        <property name="leftId" column="LEFT_ARGUMENT_ID" type="int" />
        <property name="rightId" column="RIGHT_ARGUMENT_ID" type="int" />
        <property name="isBlacklist" column="IS_BLACKLIST" type="boolean" />

    </class>
</hibernate-mapping>