<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "osd.database.BlockRecord" table = "scheduler_block">
      
      <id name = "id" type = "int" column = "ID">
         <generator class="native"/>
      </id>

      <property name="endTime" column="END_TIME" type="string" />
      <property name="startTime" column="START_TIME" type="string" />
      <property name="day" column="DAY" type="string" />
      <property name="blockId" column="BLOCK_ID" type="string" />

   </class>
</hibernate-mapping>